@model IEnumerable<MvcMusicStore.Models.Genre>

@{
    ViewBag.Title = "Store";
}

<h3>Browse Genres</h3>
<p>
    Select from @Model.Count()
    genres:
</p>

/*
creating a link for each Genre to a different page for that genre
*/
<ul>

    @foreach (var genre in Model)
    {
        <li>
            @Html.ActionLink(genre.Name,
"Browse", new { genre = genre.Name })
        </li>
    }
</ul>

@Html.ActionLink("Go to the Store Index", "Index")

